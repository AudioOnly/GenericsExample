<?xml version="1.0" encoding="utf-8"?>
<!--点击 改变 字体颜色-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:color="@android:color/holo_green_dark"/>
    <item android:state_checked="true" android:color="@android:color/holo_green_dark"/>
    <item android:state_pressed="true" android:color="@android:color/holo_green_dark"></item>
    <item android:color="@android:color/holo_green_light"/>
</selector>